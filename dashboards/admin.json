        {
          "dataset": "agrotrack",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT \r\n  t.title AS \"Task\",\r\n  (t.date + COALESCE(t.\"time\", TIME '00:00')) AS \"Due\",\r\n  CASE\r\n    WHEN (t.date + COALESCE(t.\"time\", TIME '00:00')) < NOW() THEN 'Overdue'\r\n    WHEN t.date = CURRENT_DATE THEN 'Today'\r\n    ELSE 'Upcoming'\r\n  END AS \"Priority\",\r\n  t.status AS \"Status\",\r\n  COALESCE(u.name, 'Unassigned') AS \"Assigned To\",\r\n  COALESCE(l.name, '-') AS \"Lot\",\r\n  COALESCE(a.code, '-') AS \"Animal\"\r\nFROM task t\r\nLEFT JOIN user_app u ON u.user_id = t.assigned_user_id\r\nLEFT JOIN lot l      ON l.lot_id = t.lot_id\r\nLEFT JOIN animal a   ON a.animal_id = t.animal_id\r\nWHERE t.status IN ('pending','in_progress')\r\nORDER BY \"Due\" ASC\r\nLIMIT 30;\r\n",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Tabla de tareas mejorada",
      "type": "table"
